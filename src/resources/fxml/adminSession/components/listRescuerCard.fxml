<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<Button fx:id="modifyButton" onAction="#modify" prefHeight="64.0" prefWidth="512.0"
        style="-fx-border-color: black; -fx-border-radius: 8; -fx-background-radius: 20; -fx-background-color: white;"
        xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="controller.adminSession.elements.ListRescuerCardController">
    <graphic>
        <VBox alignment="CENTER" spacing="2.0">
            <Label fx:id="nameLabel" style="-fx-font-size: 18px; -fx-text-fill: #3d5467;" text="[NOM]" />
            <Label fx:id="competencesLabel" style="-fx-font-size: 12px; -fx-text-fill: grey;" text="[Competence/Competence]" />
        </VBox>
    </graphic>
</Button>
